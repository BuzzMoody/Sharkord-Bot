{
  "name": "buzzmoody/sharkordphp",
  "replace": {
    "buzzmoody/sharkordbot": "self.version"
  },
  "description": "A ReactPHP Chatbot Framework for Sharkord",
  "type": "library",
  "version": "2.8.1",
  "require": {
    "php": "^8.5",
    "react/event-loop": "^1.6",
    "react/http": "^1.11",
    "ratchet/pawl": "^0.4.3",
    "vlucas/phpdotenv": "^5.6.3",
    "monolog/monolog": "^3.10",
    "elvanto/litemoji": "^5.2.0"
  },
  "autoload": {
    "psr-4": {
      "Sharkord\\": "src/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-dev": "2.1-dev"
    }
  }
}
